// manifest.json

{
  "manifest_version": 2,
  "name": "WhatsApp Online Tracker",
  "version": "1.0",
  "permissions": [
    "webRequest",
    "notifications"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
